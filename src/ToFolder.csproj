<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net7.0</TargetFramework>
		<ToolCommandName>ToFolder</ToolCommandName>

		<PackageId>ZKEASOFT.ToFolder</PackageId>
		<PackageVersion>1.0.1</PackageVersion>
		<Authors>ZKEASOFT</Authors>
		<Description>Move files to folder by year</Description>

		<PackAsTool>True</PackAsTool>
		<!--<GeneratePackageOnBuild>True</GeneratePackageOnBuild>-->
		<Copyright>ZKEASOFT</Copyright>
		<PackageProjectUrl>https://www.zkea.net</PackageProjectUrl>
		<RepositoryUrl>https://github.com/SeriaWei/ToFolder</RepositoryUrl>
		<PackageTags>ToFolder</PackageTags>
		<NeutralLanguage>zh-Hans</NeutralLanguage>
		<PackageIcon>icon.png</PackageIcon>
		<PackageLicense>https://www.zkea.net/licenses</PackageLicense>
		<PackageReadmeFile>README.md</PackageReadmeFile>
	</PropertyGroup>

	<ItemGroup>
		<None Include="Images\icon.png" Pack="true" PackagePath="" />
		<None Include="../README.md" Pack="true" PackagePath=""/>
	</ItemGroup>

</Project>
